<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.smktelkom.intent.MainActivity"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    >

    <Button
        android:id="@+id/pindah"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text = "Pindah Activity"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        />

    <Button
        android:id="@+id/data"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text = "Pindah Activity Dengan Data"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        />

    <Button
        android:id="@+id/objek"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text = "Pindah Activity Dengan Objek"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        />

    <Button
        android:id="@+id/dial"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text = "Telepon Nomor"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        />

    <Button
        android:id="@+id/hasil"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text = "Pindah Activity Untuk Result"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hasil Activity"
        android:textSize="20sp"
        android:gravity="center"
        />
</LinearLayout>
